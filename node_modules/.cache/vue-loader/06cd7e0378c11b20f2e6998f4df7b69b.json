picture\"\r\n          ></v-img>\r\n\r\n          <v-card-title CLASS=\"text-center mt-2 fw-bold h5\">{{ item.name }}</v-card-title>\r\n\r\n          <v-card-text>\r\n            <v-row\r\n                align=\"center\"\r\n                class=\"mx-0\"\r\n            >\r\n            </v-row>\r\n\r\n            <div class=\"my-4 text-subtitle-1\">\r\n              <b>{{ item.typeofevent }}</b>\r\n            </div>\r\n\r\n            <div class=\"my-4 text-subtitle-1\">\r\n              <b>{{ item.eventdate }}</b>\r\n            </div>\r\n\r\n\r\n            <div>{{ item.typeofevent }} prévu pour un maximum de <b>{{ item.attendance }}</b> personnes</div>\r\n          </v-card-text>\r\n\r\n            <v-card-title>{{ item.time }}</v-card-title>\r\n\r\n            <v-card-actions>\r\n            <v-btn\r\n                color=\"deep-purple lighten-2\"\r\n                text\r\n            >\r\n              Voter\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n        </v-app>\r\n\r\n      </v-carousel-item>\r\n      </v-carousel>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  name: 'Vote',\r\n  data() {\r\n    return {\r\n      name: '',\r\n      email: '',\r\n      eventkdo : '',\r\n      eventid : Number,\r\n      eventname : [],\r\n      eventdate : '',\r\n      eventattendance : Number\r\n    }\r\n  },\r\n  created() {\r\n    if (localStorage.getItem('token') === null) {\r\n      this.$router.push('/login');\r\n    }\r\n  },\r\n  mounted() {\r\n    axios.get('http://localhost:5000/user', { headers: { token: localStorage.getItem('token')}})\r\n        .then(res => {\r\n          this.name = res.data.user.name;\r\n          this.email = res.data.user.email;\r\n        })\r\n    axios.get('http://localhost:5000/event')\r\n      .then(res => {\r\n\r\n        this.eventname = res.data\r\n        console.log(this.eventname)\r\n        console.log(this.eventdate)\r\n      })\r\n\r\n\r\n  },\r\n  methods: {\r\n    logout() {\r\n      localStorage.clear();\r\n\r\n    },\r\n    retur(){\r\n        this.$router.push('/');\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n.grid-container {\r\n  display: grid;\r\n  grid-template-columns: auto auto auto auto;\r\n  justify-content: center;\r\n  gap: 30px;\r\n  column-gap: 30px;\r\n\r\n}\r\n\r\n.retur {\r\n    margin-left: 48%;\r\n}\r\n\r\n\r\n</style>\r\n"]}]}